{
    "name": "wallacemyem/lara-termii",
    "description": "A Laravel Package for Termii - https://www.termii.com",
    "keywords": [
        "wallacemyem",
        "lara-termii"
    ],
    "homepage": "https://github.com/wallacemyem/lara-termii",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Wallace Aboiyar",
            "email": "wallacemyem@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0",
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Wallacemyem\\LaraTermii\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wallacemyem\\LaraTermii\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wallacemyem\\LaraTermii\\LaraTermiiServiceProvider"
            ],
            "aliases": {
                "LaraTermii": "Wallacemyem\\LaraTermii\\Facades\\LaraTermii"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}